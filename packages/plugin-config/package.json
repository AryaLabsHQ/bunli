{
  "name": "@bunli/plugin-config",
  "version": "0.4.0",
  "type": "module",
  "description": "Config merger plugin for Bunli - loads config from multiple sources",
  "keywords": [
    "bunli",
    "cli",
    "plugin",
    "config"
  ],
  "author": "Bunli Team",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
