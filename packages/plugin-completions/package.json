{
  "name": "@bunli/plugin-completions",
  "version": "0.2.2",
  "description": "Shell completions plugin for Bunli CLI framework",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun run scripts/build.ts && bun run tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bunli/core": "workspace:*",
    "@bunli/generator": "workspace:*",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "keywords": [
    "bunli",
    "cli",
    "completions",
    "bash",
    "zsh",
    "fish",
    "shell"
  ],
  "author": "Bunli Team",
  "license": "MIT"
}
